<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>

		<!-- NuGet Info -->
		<PackageId>DeltaCore.CacheHelper</PackageId>
		<Version>1.0.1</Version>
		<Authors>Ahmed Nady</Authors>
		<Company>DeltaCore</Company>
		<Description>Lightweight memory and Redis caching helper for .NET apps</Description>
		<PackageTags>cache redis memory dotnet</PackageTags>
		<RepositoryUrl>https://github.com/AhmedNady2003/CacheHelper.git</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="9.0.4" />
		<None Include="../README.md" Pack="true" PackagePath="" />
	</ItemGroup>

</Project>
